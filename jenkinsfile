@Library('Mylib')_
pipeline
{
    agent any
    stages
    {
        stage('download')
        {
            steps
            {
                script
                {
                  cicd.gitDownload("maven")  
                }
            }
        }
        stage('build')
        {
            steps
            {
                script
                {
                    cicd.buildArtifact()
                }
            }
        }
        stage('deploy')
        {
            steps
            {
                script
                {
                    cicd.Deploy("declarativesharedlibrary","172.31.23.85","testapp")
                }
            }
        }
        stage('testing')
        {
          steps
          {
              script
              {
                  cicd.gitDownload("FunctionalTesting")  
                  cicd.runSelenium("declarativesharedlibrary")
              }
          }
        }
        stage('delivery')
        {
            steps
            {
                script
                {
                    cicd.Deploy("declarativesharedlibrary","172.31.23.210","prodapp")
                }
            }
        }
    }
}
